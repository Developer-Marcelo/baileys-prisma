    "prisma": "^7.2.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "baileys": "^7.0.0-rc.9",
    "dotenv": "^17.2.3",
