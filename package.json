{
  "name": "baileys-prisma",
  "version": "1.0.0",
  "description": "Baileys WhatsApp + Prisma ORM: gerenciamento de sessões e multi-instâncias com persistência em PostgreSQL | Session management and multi-instance support",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npx tsx src/main.ts",
    "start": "npx tsx baileys-prisma/index.js",
    "build": "npx tsc && tsc-alias"
  },
  "keywords": [
  "baileys",
  "baileys-prisma",
  "whatsapp",
  "whatsapp-bot",
  "bot-whatsapp",
  "automacao-whatsapp",
  "whatsapp-api",
  "api-whatsapp",
  "whatsapp-js",
  "whatsapp-session",
  "sessao-whatsapp",
  "whatsapp-multidevice",
  "multi-instancia",
  "multi-instance",
  "baileys-wrapper",
  "wrapper-whatsapp",
  "prisma",
  "orm",
  "postgresql",
  "database",
  "typescript",
  "nodejs",
  "whatsapp-automation"
],
"homepage": "https://github.com/Developer-Marcelo/baileys-prisma#readme",
"bugs": {
  "url": "https://github.com/Developer-Marcelo/baileys-prisma/issues"
},
  "author": "Marcelo P. S. P. <desenvolvedoreasycode@gmail.com> (https://github.com/developer-marcelo)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Developer-Marcelo/baileys-prisma"
  },
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/qrcode": "^1.5.6",
    "prisma": "^7.2.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "baileys": "^7.0.0-rc.9",
    "dotenv": "^17.2.3",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "qrcode": "^1.5.4"
  }
}
